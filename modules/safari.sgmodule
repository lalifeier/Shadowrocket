[URL Rewrite]
# Safari 内输入 命令 + 空格 + 关键词 快速指定搜索引擎搜索
#  注：先进入设置更改 Safari 默认搜索为 百度

# 翻译
# yd  xxx (有道词典)
^https:\/\/m.baidu.com\/s\/\?wd=yd\+([^&]+).+ http://dict.youdao.com/search?q=$1 302
^https:\/\/m.baidu.com\/s\/\?wd=([^+]+)\+yd.+ http://dict.youdao.com/search?q=$1 302
# trc xxx (Google 译至中)
^https:\/\/m.baidu.com\/s\/\?wd=trc\+([^&]+).+ https://translate.google.com/#view=home&op=translate&sl=auto&tl=zh-CN&text=$1 302
^https:\/\/m.baidu.com\/s\/\?wd=([^+]+)\+trc.+ https://translate.google.com/#view=home&op=translate&sl=auto&tl=zh-CN&text=$1 302
# tre xxx (Google 译至英)
^https:\/\/m.baidu.com\/s\/\?wd=tre\+([^&]+).+ https://translate.google.com/#view=home&op=translate&sl=auto&tl=en&text=$1 302
^https:\/\/m.baidu.com\/s\/\?wd=([^+]+)\+tre.+ https://translate.google.com/#view=home&op=translate&sl=auto&tl=en&text=$1 302
